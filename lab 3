             LAB MANUAL-3

SQL> CREATE TABLE Emp01
  2  (
  3  eno number(3),
  4  ename varchar(30),
  5  dept varchar(50),
  6  salary number
  7  );

Table created.

SQL> INSERT INTO Emp01 VALUES (1,'RAJU','PHILOSOPHY',90000);

1 row created.

SQL> INSERT INTO Emp01 VALUES (2,'AKILA','ENGLISH',80000) ;

1 row created.

SQL> INSERT INTO Emp01 VALUES (3,'PADMA','HISTORY',60000) ;

1 row created.

SQL> INSERT INTO Emp01 VALUES (4,'SANJAY','MECH',100000) ;

1 row created.

SQL> INSERT INTO Emp01 VALUES (5,'LATHA','BIOLOGY',50000) ;

1 row created.

