SQL> SELECT avg (salary) "average salary" FROM Emp01 ;

average salary
--------------
         76000

SQL> SELECT min (salary) "minimum salary" FROM Emp01 ;

minimum salary
--------------
         50000

SQL> SELECT max (salary) "maximum salary" FROM Emp01 ;

maximum salary
--------------
        100000

SQL> SELECT count (eno) "number of employees" FROM Emp01 ;

number of employees
-------------------
                  5

SQL> SELECT sum (salary) "number of salary" FROM Emp01 ;

number of salary
----------------
          380000

